{
  "version": 2,
  "framework": null,
  "buildCommand": "",
  "outputDirectory": "frontend",
  "public": true,
  "cleanUrls": true,
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/widget.js",
      "dest": "/widget.js"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}